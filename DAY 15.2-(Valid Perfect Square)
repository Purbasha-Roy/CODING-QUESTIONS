//Valid Perfect Square
    bool isPerfectSquare(int num) {
        bool output=false;
        long long i=1;
        while(i<=num){
            long long mult=i*i;
            if(mult==num){
                output=true;
            }
            else if(mult>num){
                break;
            }
            i++;
        }
        return output;
    }
