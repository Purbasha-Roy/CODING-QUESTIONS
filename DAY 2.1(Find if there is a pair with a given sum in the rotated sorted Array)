//Find if there is a pair with a given sum in the rotated sorted Array
//METHOD 1
#include<iostream>
using namespace std;
bool pairSum(int arr[],int n,int key)
{
    if(n==0 || n==1)
    {
        return false;
    }
    for(int i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(arr[i]+arr[j]==key){
                return true;
            }
        }
    }
    return false;
}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int key;
    cin>>key;
    if(pairSum(arr,n,key)){
        cout<<"True"<<endl;
    }
    else{
        cout<<"False"<<endl;
    }
    return 0;
}

//METHOD 2
#include<iostream>
#include<algorithm>
using namespace std;
bool pairSum(int arr[],int n,int key)
{
    sort(arr,arr+n);
    int start=0;
    int end=n-1;
    while(start<end)
    {
        int sum=arr[start]+arr[end];
        if(arr[start]+arr[end]==key){
            return true;
        }
        else if(sum>key){
            end--;
        }
        else{
            start++;
        }
    }
    return false;
}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int key;
    cin>>key;
    if(pairSum(arr,n,key)){
        cout<<"True"<<endl;
    }
    else{
        cout<<"False"<<endl;
    }
    return 0;
}
